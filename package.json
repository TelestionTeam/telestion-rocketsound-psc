{
	"name": "telestion-rocketsound-psc",
	"description": "telestion-rocketsound-psc, a Telestion PSC generated using the telestion-client-cli",
	"version": "0.0.0",
	"homepage": ".",
	"scripts": {
		"start": "npm run start:electron",
		"start:electron": "tc-cli start --electron",
		"start:browser": "tc-cli start --browser",
		"build": "tc-cli build",
		"stats": "tc-cli stats",
		"style": "prettier --write package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'",
		"style:ci": "prettier --list-different package.json '**/*{.ts,.tsx,.js,.jsx,.json,.html,.css,.scss,.sass,.yaml,.yml,.md}'"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"private": true,
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"dependencies": {
		"@adobe/react-spectrum": "^3.8.0",
		"@react-spectrum/table": "^3.0.0-alpha.9",
		"@react-spectrum/tabs": "^3.0.0-alpha.4",
		"@spectrum-icons/illustrations": "^3.2.1",
		"@spectrum-icons/ui": "^3.2.0",
		"@spectrum-icons/workflow": "^3.2.0",
		"@wuespace/telestion-client-common": "^0.11.1",
		"@wuespace/telestion-client-core": "^0.11.0",
		"@wuespace/telestion-client-prop-types": "^0.11.0",
		"@wuespace/vertx-mock-server": "^0.11.0",
		"electron": "^11.3.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0",
		"zustand": "^3.3.2"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.5",
		"@testing-library/user-event": "^12.7.3",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.31",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"@wuespace/telestion-client-cli": "^0.11.0",
		"@wuespace/telestion-client-types": "^0.11.0",
		"husky": "^4.3.8",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"react-scripts": "^4.0.3",
		"typescript": "^4.2.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
